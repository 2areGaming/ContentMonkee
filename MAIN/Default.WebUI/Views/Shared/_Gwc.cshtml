@model dynamic

@using Newtonsoft.Json
@using Newtonsoft.Json.Serialization

@{
    Layout = null;
}

<script>
    var gwc = {};
    window.gwc = gwc;
    @if(Model!=null)
    {
        //var json = Default.WebUI.Extensions.JsonExtensions.ToJson(Model);
        ////gwc.model = @Html.Raw(json);
        <text>
    gwc.serverpath = "@(Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/'))";
    </text>
    }
    @{
        if (Model is AdminViewModel || Model is AdminWidgetViewModel)
        {
            if(Model.CurrentSiteSetting != null) {
            <text>
    gwc.sitesettingpath = "@("Content/" + Model.CurrentSiteSetting.Id)";
    </text>
            }
        }
    }
</script>